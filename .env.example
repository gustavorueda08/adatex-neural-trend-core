# Environment (DEV or PROD)
ANTC_ENV=DEV

# Storage (AWS S3) - Required if ANTC_ENV=PROD
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
AWS_S3_BUCKET=antc-assets-bucket

# Database (PostgreSQL) - Required if ANTC_ENV=PROD
# Format: postgresql://user:password@localhost:5432/dbname
DATABASE_URL=

# Intelligence (Google Gemini)
GOOGLE_API_KEY=

# Optional: Headless Browser Config
SELENIUM_HEADLESS=true
